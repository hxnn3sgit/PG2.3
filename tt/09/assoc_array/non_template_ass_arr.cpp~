class associative_array {
private:
	std::pair<int, string> *data;	
	unsigned int size;
public:
	associative_array() : data(nullptr), size(0) {}
	associative_array(int size) : data(new std::pair<int, string>[size]), size(size) {} 
	//associative_array() noch anderer konstruktor???
	~associative_array();
